Type tMBR
    sLowerLeft     As String
    sUpperRight    As String
End Type

Type tGeoSet
    sAutoLayer     As String
    sCenter        As String
    sCenter_User   As String
    sMapUnit       As String
    vtMBR          As tMBR
    sName          As String
    sProjection    As String
    sZoomLevel     As String
End Type

Type tBrush
    sBackColor     As String
    sForeColor     As String
    sPattern       As String
    sTransparent   As String
End Type

Type tPen
    sColor         As String
    sLineStyle     As String
    sLineWidth     As String
End Type

Type tFont
    sBackColor     As String
    sDescription   As String
    sExtStyle      As String
    sForeColor     As String
    sOpaque        As String
    sSize          As String
    sStyle         As String
    sRotation      As String
End Type

Type tBitmap
    sName          As String
    sOverrideColor As String
    sTransparent   As String
End Type

Type tSymbol
    sCode          As String
    sColor         As String
    vtFont         As tFont
    sPointSize     As String
    sType          As String
    vtBitmap       As tBitmap
End Type

Type tZoom
    sMin           As String
    sMax           As String
    sUnit          As String
End Type

Type tDisplay
    vtBrush        As tBrush
    vtPen          As tPen
    vtLinePen      As tPen
    vtFont         As tFont
    vtSymbol       As tSymbol
End Type

Type tLabel
    sDuplicate    As String
    vtFont        As tFont
    sLineType     As String
    sMaxLabels    As String
    sOffset       As String
    sOverlap      As String
    sParallel    As String
    sPartialSegments As String
    sPosition     As String
    vtZoom        As tZoom
End Type

Type tGeoSetTable
    sTableName    As String
    sRealName     As string
    sAutoLabel    As String
    vtDisplay     As tDisplay
    sFile         As String
    sIsVisible    As String
    vtLabel       As tLabel
    sSelectable   As String
    sEditable     As String
    vtZoom        As tZoom
    sShowLineDirection As String
    sShowNodes    As String
    sShowCentroids As String
    sRegisteringGeoDict As String
    sDescription  As String
    sDrawLabelsAfter As String
End Type




